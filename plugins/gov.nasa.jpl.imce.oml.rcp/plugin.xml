<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="OML Workbench"
            application="org.eclipse.ui.ide.workbench">
         <property
               name="appName"
               value="OML Workbench">
         </property>
         <property
               name="aboutText"
               value="OML Workbench&#x0A;&#x0A;(c) Copyright Jet Propulsion Laboratory 2018.  All rights reserved.&#x0A;&#x0A;This product includes software developed by open source projects including the Eclipse Organization, https://www.eclipse.org/, and the Apache Software Foundation, https://www.apache.org/.">
         </property>
         <property
               name="aboutImage"
               value="images/about.png">
         </property>
         <property
               name="windowImages"
               value="images/eclipse16.png,images/eclipse32.png,images/eclipse48.png,images/eclipse64.png,images/eclipse128.png,images/eclipse256.png">
         </property>
      </product>
   </extension>

</plugin>
